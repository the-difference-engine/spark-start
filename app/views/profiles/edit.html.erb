Edit Profile

<%= form_tag '/profile/#{@profile.id}', method: :patch do %>
  <p><%= label_tag :experience %>
  <%= text_field_tag :experience %></p>

  <p><%= label_tag :bio %>
  <%= text_field_tag :bio %></p>

  <p><%= label_tag :phone %>
  <%= text_field_tag :phone %></p>

  <p><%= label_tag :career %>
  <%= text_field_tag :career %></p>

  <p><%= label_tag :image %>
  <%= text_field_tag :image %></p>

  <p><%= label_tag :user_id %>
  <%= text_field_tag :user_id %></p>

  <%= submit_tag "Submit" %>

<% end %>
